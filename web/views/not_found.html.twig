                        </div>
                    </div>
     
        </div>
      </div>
    </div>


    {% endblock %}

    {% block javascripts %}
    <!-- jQuery 2.0.2 -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="{{ app.asset_path }}/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- iCheck -->



    {% endblock %}